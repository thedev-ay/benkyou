{
    "name": "Benkyou",
    "description": "Benkyou",
    "version": "0.1",
    "manifest_version": 2,
    "permissions": [ "storage" ],
    "icons": {
      "48": "/assets/icon_48.png",
      "128": "/assets/icon_128.png"
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "browser_action": {
      "default_title": "Benkyou"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }